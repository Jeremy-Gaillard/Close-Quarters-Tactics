<?xml version="1.0" encoding="UTF-8"?>
<!-- 
	ROT = Rate Of Fire, given in RPM (round per minute)
	Imprecision = fraction of radians (eg for shotgun: imprecision = pi/10 radians)
	Reload Time : in seconds
-->
